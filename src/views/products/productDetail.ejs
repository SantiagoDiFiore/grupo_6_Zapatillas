        <section class="main-article-product">
            <div class="imagen-principal">
                <img src="/images/adidas1.png" width="100%" alt="Adidas Ozweego Orange" class="main-product-image">
            </div>
            <div class="data-principal">
                <div class="product-body">
                    <h2 class="main-title">Adidas Ozweego Orange (Youth)</h2>
                    <p class="precio-pd">$13.999</p>
                    <h3 class="talle">Elegi tu talle</h3>
                    <div class="talle1">
                        <p class="guia-talles"><a href=# class="cfont-talles"><i
                                    class="fas fa-ruler-horizontal"></i>Guia de talles</a></p>
                        <form action="/talle" class="tallesdisponibles">
                            <input type="button" name="talle" value="39" class="boton-talle">
                            <input type="button" name="talle" value="40" class="boton-talle">
                            <input type="button" name="talle" value="41" class="boton-talle">
                            <input type="button" name="talle" value="42" class="boton-talle">
                            <input type="button" name="talle" value="43" class="boton-talle">
                            <input type="button" name="talle" value="44" class="boton-talle">
                        </form>
                    </div>
                    <div class="cantidad1">
                        <p class="cantidad-titulo">Cantidad:</p>
                        <form action="/cantidad" class="cantidaditem">
                            <select name="cantidad">
                                <option value="1">1</option>
                                <option value="2">2</option>
                                <option value="3">3</option>
                                <option value="4">4</option>
                                <option value="5">5</option>
                            </select>
                        </form>
                    </div>
                    <div class="realizar-compra">
                        <a href="productCart" class="boton-de-compra">Compra<i class="fas fa-shopping-bag"></i></a>

                    </div>
                    <div class="enviosgratis">
                        <p class="envios1"><i class="fas fa-shipping-fast"></i>¡ENVIOS SIN CARGO A TODO EL PAÍS!</p>
                    </div>
                </div>
